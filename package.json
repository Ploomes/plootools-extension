{
  "name": "plootools",
  "displayName": "PlooTools",
  "description": "Create components and tests simply and intuitively.",
  "version": "0.0.15",
  "publisher": "UismaLopes",
  "icon": "img/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ploomes/plootools.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "author": {
    "name": "Uisma Lopes"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.72.0"
  },
  "homepage": "https://github.com/Ploomes/plootools",
  "categories": [
    "Other"
  ],
  "keywords": [
    "ploomes",
    "CRM",
    "react",
    "jest",
    "tests",
    "component"
  ],
  "activationEvents": [
    "workspaceContains:.plootools"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "plootools.menu",
        "title": "PlooTools"
      },
      {
        "command": "plootools.init",
        "title": "PlooTools init"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "group": "plootools",
          "title": "PlooTools",
          "command": "plootools.menu"
        },
        {
          "group": "plootools",
          "title": "PlooTools init",
          "command": "plootools.init"
        }
      ]
    },
    "snippets": [
      {
        "language": "typescriptreact",
        "path": "./out/snippets/ts/tsx.json"
      },
      {
        "language": "typescript",
        "path": "./out/snippets/ts/ts.json"
      },
      {
        "language": "javascript",
        "path": "./out/snippets/test.json"
      },
      {
        "language": "snippets",
        "path": "./out/snippets/test.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "npm run clean && tsc -p ./ && npm run copy:files",
    "watch": "npm run clean && npm run copy:files && tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js",
    "copy:files": "node ./cp.js",
    "clean": "rm -rf out",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.17.15",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/prettier": "^2.7.1",
    "@types/resolve": "^1.20.6",
    "@types/vscode": "^1.72.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "tsc-alias": "^1.8.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "fs": "^0.0.1-security",
    "lodash": "^4.17.21",
    "path": "^0.12.7",
    "prettier": "^3.6.2",
    "resolve": "^1.22.10"
  }
}
